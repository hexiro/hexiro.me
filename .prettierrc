{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "importOrder": [
        "((^@fontsource)|(.css$))",
        "^(react|next)(/.*|)$",
        "^(@chakra-ui)(/.*|)$",
        ".*"
    ],
    "importOrderSeparation": true,
    "quoteProps": "preserve",
    "arrowParens": "avoid"
}
